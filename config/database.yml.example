default: &default
  adapter: mysql2
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  timeout: 5000
  host: host
  username: username
  password: password

development:
  <<: *default
  database: devise_development

test:
  <<: *default
  database: devise_test

production:
  <<: *default
  database: db/production
